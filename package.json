{
  "name": "ecart-seller-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "build": "run-s clean build:start copy:gql",
    "build:start": "tsc && tsc-alias",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "format:watch": "onchange \"**/*\" --exclude-path .prettierignore -- prettier --write {{changed}}",
    "copy:gql": "copyfiles -u 1 src/**/*.gql  dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logsbydeepak/ecart-seller-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/logsbydeepak/ecart-seller-backend/issues"
  },
  "homepage": "https://github.com/logsbydeepak/ecart-seller-backend#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.4",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@swc/core": "^1.2.207",
    "@swc/helpers": "^0.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cryptr": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/validator": "^13.7.3",
    "copyfiles": "^2.4.1",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.6.11",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.0",
    "apollo-server-express": "^3.9.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.0",
    "cookie-parser": "^1.4.6",
    "cryptr": "^6.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "mongoose": "^6.4.1",
    "redis": "^4.1.0",
    "validator": "^13.7.0",
    "winston": "^3.8.0",
    "winston-mongodb": "^5.0.7"
  }
}
