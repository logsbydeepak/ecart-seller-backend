{
  "name": "ecart-seller-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev run-s clean dev:start",
    "dev:start": "tsnd -r tsconfig-paths/register --rs --cache-directory \"./dist\" -- src/server.ts",
    "build": "run-s clean build:start copy:gql",
    "build:start": "tsc",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "format:watch": "onchange \"**/*\" --exclude-path .prettierignore -- prettier --write {{changed}}",
    "copy:gql": "copyfiles -u 1 src/**/*.gql  dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logsbydeepak/ecart-seller-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/logsbydeepak/ecart-seller-backend/issues"
  },
  "homepage": "https://github.com/logsbydeepak/ecart-seller-backend#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.4",
    "@graphql-tools/load": "^7.5.2",
    "@graphql-tools/schema": "^8.3.2",
    "apollo-server-express": "^3.6.3",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "mongoose": "^6.2.4",
    "tsconfig-paths": "^3.13.0"
  }
}
